<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
        xmlns:fc="http://ns.adobe.com/flashcatalyst/2009"
        height="13" >
    
    <fx:Metadata>
        [HostComponent("spark.components.Button")]
    </fx:Metadata> 
    
    <s:states>
        <s:State name="up" />
        <s:State name="down" />
        <s:State name="over" />
        <s:State name="disabled" />
    </s:states>
    
    <s:Rect left="0" right="0" top="0" bottom="0">
        <s:stroke>
            <s:SolidColorStroke alpha="0.6" caps="square" color="#000000" joints="miter"/>
        </s:stroke>
    </s:Rect>
    <s:Rect left="1" right="1" height="11">
        <s:fill>
            <s:SolidColor alpha="0.2" color="#333333"/>
        </s:fill>
    </s:Rect>
    <s:Rect left="1" right="1" top="1" bottom="1">
        <s:fill>
            <s:SolidColor alpha="0.2" color="#000000"/>
        </s:fill>
    </s:Rect>
    <s:Rect left="1" right="1" top="1" bottom="1">
        <s:stroke>
            <s:SolidColorStroke alpha="0.08" caps="square" color="#000000" joints="miter"/>
        </s:stroke>
    </s:Rect>
    <s:Rect x="1" top="1" bottom="1" width="1">
        <s:fill>
            <s:SolidColor alpha="0.15" color="#000000"/>
        </s:fill>
    </s:Rect>
    
</s:Skin>
