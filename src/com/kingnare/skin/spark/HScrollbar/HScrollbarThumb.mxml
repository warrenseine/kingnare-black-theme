<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:s="library://ns.adobe.com/flex/spark"
        xmlns:fc="http://ns.adobe.com/flashcatalyst/2009"
        xmlns:d="http://ns.adobe.com/fxg/2008/dt"
        width="30" height="13">
    
    <fx:Metadata>
        [HostComponent("spark.components.Button")]
    </fx:Metadata> 
    
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
    <s:Rect y="0.5" width="29" height="12" left="0" right="0">
        <s:stroke>
            <s:SolidColorStroke alpha="0.6" caps="square" color="#000000" joints="miter"
                                color.disabled="#333333"/>
        </s:stroke>
    </s:Rect>
    <s:Rect y="1.5" width="27" height="10" left="1" right="1">
        <s:stroke>
            <s:SolidColorStroke alpha="0.15" alpha.over="0.23" caps="square" color="#FFFFFF" joints="miter"/>
        </s:stroke>
    </s:Rect>
    <s:Rect y="2" width="26" height="9" d:locked="true" left="2" right="2">
        <s:fill>
            <s:SolidColor alpha="0.05" alpha.over="0.10" color="#FFFFFF"/>
        </s:fill>
    </s:Rect>
    <s:Group horizontalCenter="0.5" verticalCenter="0.5">
        <s:Rect x="0" y="0" width="1" height="3">
            <s:fill>
                <s:SolidColor color="#FFFFFF"
                              alpha.disabled="0.5"
                              alpha.down="0.5"
                              alpha.up="0.5"/>
            </s:fill>
        </s:Rect>
        <s:Rect x="2" y="0" width="1" height="3">
            <s:fill>
                <s:SolidColor color="#FFFFFF"
                              alpha.disabled="0.5"
                              alpha.down="0.5"
                              alpha.up="0.5"/>
            </s:fill>
        </s:Rect>
        <s:Rect x="4" y="0" width="1" height="3">
            <s:fill>
                <s:SolidColor color="#FFFFFF"
                              alpha.disabled="0.5"
                              alpha.down="0.5"
                              alpha.up="0.5"/>
            </s:fill>
        </s:Rect>
        <s:Rect x="0" y="3" width="1" height="1">
            <s:fill>
                <s:SolidColor alpha="0.3" color="#000000"/>
            </s:fill>
        </s:Rect>
        <s:Rect x="2" y="3" width="1" height="1">
            <s:fill>
                <s:SolidColor alpha="0.3" color="#000000"/>
            </s:fill>
        </s:Rect>
        <s:Rect x="4" y="3" width="1" height="1">
            <s:fill>
                <s:SolidColor alpha="0.3" color="#000000"/>
            </s:fill>
        </s:Rect>
    </s:Group>
    
    <!-- background -->
    <!-- Gripper -->
    <!-- shadow line -->
</s:Skin>
